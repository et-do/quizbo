{
    "name": "Article-Ally DevContainer",
    "dockerFile": "./Dockerfile",
    "context": "..",
    "customizations": {
      "vscode": {
        "extensions": [
          "ms-python.python",
          "dbaeumer.vscode-eslint",
          "esbenp.prettier-vscode"
